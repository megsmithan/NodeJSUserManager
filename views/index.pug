html
    head
        style
            include ../style.css
        title= title
    body
        h1 users
        a.button(href='/form') Add New User
        div.container

            table
                    each user in message
                        tr
                            td= user.username
                            td= user.name
                            td= user.email
                            td= user.age
                    a.editBtn(href='edit') edit




